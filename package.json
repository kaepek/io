{
  "name": "kaepek-io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "node_version": "v18.12.1",
  "scripts": {
    "test": "ts-node ./lib/host/controller/scripts/test.ts",
    "build": "tsc --project tsconfig.json",
    "cli": "node --loader ts-node/esm ./dist/host/controller/scripts/director.js",
    "cli2": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 node --experimental-loader=extensionless ./dist/host/controller/scripts/director.js",
    "cli3": "npm run cli2 -- -i network=localhost,9090,udp -i keyboard -c start -c null -c stop -p serial-usb -p console -o console",
    "cli-test": "ts-node lib/host/controller/scripts/cli.ts -i network=localhost,9090,udp -i keyboard -c start -c null -c stop -p serial-usb -p console -o console",
    "graph": "bokeh serve --port 8033 --show lib/graphing/plot.py "
  },
  "author": "Jonathan Kelsey",
  "license": "AGPL3",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dualshock": "github:jk89/dualshock",
    "extensionless": "^1.4.5",
    "readline": "^1.3.0",
    "rxjs": "^7.8.1",
    "serialport": "^10.4.0",
    "ts-node": "^10.9.1",
    "typed-struct": "github:jk89/typed-struct"
  },
  "bin": {
    "kaepek-io-director": "./dist/host/controller/scripts/director.js",
    "kaepek-io-director2": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 node --experimental-loader=extensionless ./dist/host/controller/scripts/director.js",
    "kaepek-torque-delay-direction-dualshock-serial": "./lib/models/torque-delay-direction/scripts/dualshock-serial.js",
    "kaepek-torque-delay-direction-dualshock-console": "./lib/models/torque-delay-direction/scripts/dualshock-console.js",
    "kaepek-torque-phase-direction-dualshock-serial": "./lib/models/torque-phase/scripts/dualshock-serial.js",
    "kaepek-torque-phase-direction-dualshock-console": "./lib/models/torque-phase/scripts/dualshock-console.js",
    "kaepek-ThrustU16LEDirectionU8Model-dualshock-serial": "./lib/models/thrustU16LE-directionU8/scripts/dualshock-serialport.js",
    "kaepek-ThrustU16LEDirectionU8Model-dualshock-serial-graph": "./lib/models/thrustU16LE-directionU8/scripts/dualshock-serialport-graph.js",
    "kaepek-ThrustU16LEDirectionU8Model-dualshock-console": "./lib/models/thrustU16LE-directionU8/scripts/dualshock-console.js"
  },
  "devDependencies": {
    "@types/node": "^20.5.4"
  }
}
